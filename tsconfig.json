{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "outDir": "dist",
      "strict": true,
      "rootDir": "./src",
      "noEmitOnError": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      // "declaration": true,
      // "sourceMap": true,
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }
  